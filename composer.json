{
    "name": "draperstudio/eloquent-hashid",
    "type": "library",
    "description": "Adds Hashid support to Eloquent models",
    "keywords": [
        "draperstudio",
        "laravel-hashid"
    ],
    "homepage": "https://github.com/DraperStudio/laravel-eloquent",
    "license": "MIT",
    "authors": [{
        "name": "DraperStudio",
        "email": "hello@draperstudio.tech",
        "homepage": "http://draperstudio.tech",
        "role": "Developer"
    }],
    "require": {
        "php": "^5.6 || ^7.0",
        "hashids/hashids": "^1.0",
        "illuminate/database": "5.1.* || 5.2.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "phpunit/phpunit": "^5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "DraperStudio\\Eloquent\\Hashid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DraperStudio\\Tests\\Eloquent\\Hashid\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
